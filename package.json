{
  "name": "ICAN",
  "version": "1.0.0",
  "description": "ICAN website",
  "main": "app/client/src/scripts/app.js",
  "repository": "git@github.com:trainordigital/ICAN.git",
  "author": "Brandon Iffert <brandoniffert@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack",
    "analyze": "cross-env yarn run build && webpack-bundle-analyzer app/client/dist/stats.json"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": "core-js@3"
        }
      ]
    ]
  },
  "browserslist": [
    ">0.25%",
    "not ie 10",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jquery": true
    },
    "globals": {
      "Selectors": true,
      "ajax": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "no-console": 1
    }
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "core-js": "^3.1.4",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-pxtorem": "^4.0.1",
    "sass-loader": "7.1.0",
    "svg-spritemap-webpack-plugin": "^3.2.2",
    "url-loader": "^1.1.1",
    "webpack": "^4.29.6",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "axios": "^0.21.1",
    "body-scroll-lock": "^2.6.1",
    "countup.js": "^2.0.3",
    "delegated-events": "^1.0.10",
    "easydropdown": "^4.1.1",
    "flickity": "^2.2.0",
    "flickity-fade": "^1.0.0",
    "flickity-sync": "^2.0.0",
    "foundation-sites": "^6.5.3",
    "js-cookie": "^2.2.0",
    "jump.js": "^1.0.2",
    "lg-thumbnail.js": "^1.1.0",
    "lightgallery.js": "^1.1.3",
    "lightpick": "1.2.11",
    "lodash": "^4.17.11",
    "object-fit-images": "^3.2.4",
    "pristinejs": "^0.1.6",
    "rfs": "^8.0.2",
    "tingle.js": "^0.14.0",
    "verge": "^1.10.2"
  }
}
